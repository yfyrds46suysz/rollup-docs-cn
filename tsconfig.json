{
    "compilerOptions": {
        "diagnostics": true,
        "lib": ["es5", "es6", "dom"],
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true
    },
    "target": "ES5",
    "include": [
        "src",
        "bin"
    ],
    "exclude": [
        "node_modules"
    ]
}
